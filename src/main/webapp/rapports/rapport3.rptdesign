            <property name="queryText">select Accueil.acc_id, Accueil.acc_date,Accueil.acc_commentaire,
	   AttributCategorie.att_libelle, Categorie.cat_libelle,TypePublic.pub_libelle, 
	   TypeReception.rec_libelle,User.login,Groupe.nom_groupe
from Accueil,AttributCategorie,Categorie,TypeReception,TypePublic,User,Groupe
where Accueil.acc_typereception =TypeReception.rec_id
and Accueil.acc_typereponse =AttributCategorie.att_id
and Accueil.acc_typepublic = TypePublic.pub_id
and Categorie.cat_id = AttributCategorie.att_categorie
and User.id=Accueil.id_agent
and Groupe.group_id=Categorie.groupe_id</property>
