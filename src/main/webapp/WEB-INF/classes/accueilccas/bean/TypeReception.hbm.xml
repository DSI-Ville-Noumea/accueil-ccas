<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="accueilccas.bean.TypeReception" table="TypeReception">
      	<id name="idTypeReception" column="rec_id">
      		<generator class="native" />
      	</id>
 	 	
      	<property name="libelleTypeReception" column="rec_libelle"/> 	
      	<property name="positionTypeReception" column="rec_position"/> 
      	<property name="enabled" column="rec_enabled"/>
	</class> 
</hibernate-mapping>